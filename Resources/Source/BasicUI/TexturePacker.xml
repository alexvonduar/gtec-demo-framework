<?xml version="1.0" encoding="UTF-8"?>
<TexturePacker xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Version="1">
  <AtlasFlavors Name="UIAtlas/UIAtlas">
    <CreateAtlas Name="UIAtlas/UIAtlas_80dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font12/Font.fnt"/>
    </CreateAtlas>
    <CreateAtlas Name="UIAtlas/UIAtlas_96dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font14/Font.fnt"/>
    </CreateAtlas>
    <CreateAtlas Name="UIAtlas/UIAtlas_160dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font24/Font.fnt"/>
    </CreateAtlas>
    <CreateAtlas Name="UIAtlas/UIAtlas_320dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font48/Font.fnt"/>
    </CreateAtlas>
    <CreateAtlas Name="UIAtlas/UIAtlas_480dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font72/Font.fnt"/>
    </CreateAtlas>
    <CreateAtlas Name="UIAtlas/UIAtlas_640dpi">
      <AddFolder Path="UIAtlas/${Atlas.DefaultDpi}"/>
      <AddBitmapFont Path="Font/Font96/Font.fnt"/>
    </CreateAtlas>
  </AtlasFlavors>
  <CreateAtlas Name="Font/NormalAtlas/FontAtlas">
    <AddBitmapFont Path="Font/Normal/Font.fnt" Type="bitmap" OutputFontFormat="fbk,nbf"/>
  </CreateAtlas>
  <CreateAtlas Name="Font/SdfAtlas/FontAtlas" TransparencyMode="normal">
    <AddBitmapFont Path="Font/Sdf/Font.fnt" Type="sdf" OutputFontFormat="fbk,nbf"/>
  </CreateAtlas>
</TexturePacker>
