<?xml version="1.0" encoding="UTF-8"?>
<FslBuildGen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../FslBuildGen.xsd">
  <!-- Define the general OpenCV concept, warning this does not pull in any libraries by itself -->
  <ExternalLibrary Name="OpenCV" CreationYear="2016">
    <Default.Platform.Supported Value="false"/>
    <Requirement Name="OpenCV" Type="feature"/>
    <Requirement Name="EarlyAccess" Type="feature"/>
    <Define Name="FSL_FEATURE_OPENCV" Access="Public"/> 
    <Platform Name="Ubuntu" Supported="true"/>
    <Platform Name="Windows" Supported="true"/>
    <Platform Name="Yocto" Supported="true"/>
  </ExternalLibrary>
</FslBuildGen>
