<?xml version="1.0" encoding="UTF-8"?>
<FslBuildGen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../FslBuildGen.xsd">
  <ToolRecipe Name="Recipe.BuildTool.clang_tidy" CreationYear="2017">
    <Dependency Name="Recipe.BuildTool.clang"/>
    <ExperimentalRecipe>
      <Installation>
        <FindExecutableFileInPath Name="clang-tidy" Alternatives="clang-tidy-10" MinVersion="10.0" VersionCommand="-version" VersionRegEx="LLVM version\s*([\d.]+)" Help="Please install clang-tidy 10.0+ as its needed for tidy (http://releases.llvm.org/download.html)"/>
        <FindExecutableFileInPath Name="clang-apply-replacements" Alternatives="clang-apply-replacements-10" MinVersion="10.0" VersionCommand="-version" VersionRegEx="version\s*([\d.]+)" Help="Please install clang-tidy 10.0+ as its needed for tidy (http://releases.llvm.org/download.html)"/>
      </Installation>
    </ExperimentalRecipe>
  </ToolRecipe>
</FslBuildGen>
